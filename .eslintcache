 <ListItemText\n          className='appointment-secondary appointment-secondary__control'\n          primary={entry?.control}\n        />\n        <ListItemText\n          className='appointment-secondary appointment-secondary__technician'\n          primary={entry?.technician}\n        />\n        <ListItemText className='appointment-secondary appointment-secondary__price' primary={entry?.price} />\n        <div className='appointment-secondary appointment-secondary__action-buttons'>\n          <FaPen\n            className='appointment-secondary appointment-secondary__edit-icon'\n            onClick={() => setShowEditModal(true)}\n          />\n          <FaTrashAlt\n            className='appointment-secondary appointment-secondary__remove-icon'\n            onClick={() => removeEntry()}\n          />\n        </div>\n      </ListItem>\n      {showEditModal && (\n        <Dialog\n          type='appointment'\n          action='edit'\n          values={formValues}\n          setShowModal={setShowEditModal}\n          successHandler={() => {}}\n        />\n      )}\n    </Fragment>\n  );\n};\n\nexport default AppointmentSecondary;\n","/Users/alexandru.andronache/_work/client-control-panel/src/components/Appointment/Appointment.js",[],"/Users/alexandru.andronache/_work/client-control-panel/src/components/AppointmentList/AppointmentList.js",[],{"ruleId":"116","severity":1,"message":"117","line":7,"column":32,"nodeType":"118","messageId":"119","endLine":7,"endColumn":55},{"ruleId":"120","severity":1,"message":"117","line":7,"column":32,"nodeType":"118","messageId":"119","endLine":7,"endColumn":55},{"ruleId":"116","severity":1,"message":"121","line":28,"column":45,"nodeType":"118","messageId":"119","endLine":28,"endColumn":54},{"ruleId":"120","severity":1,"message":"121","line":28,"column":45,"nodeType":"118","messageId":"119","endLine":28,"endColumn":54},{"ruleId":"116","severity":1,"message":"122","line":28,"column":56,"nodeType":"118","messageId":"119","endLine":28